{% extends 'FklFranklinBundle::base.html.twig' %}
{% block stylesheets %}
{% endblock %}  

{% block body %}
<h1>Suivi de commande</h1>
<hr>

<div id="suivicommand">
    <h3>Votre Commande :</h3>
    <form  class="form-inline" method="POST" role="form">
        <input type="text" name="sku" placeholder="RÃ©frence ex: EXV56DD" class="form-control input-box">
        <button type="submit" class="btn btn-default">Rechercher</button>
    </form>

    <h3>Etat de la commande :</h3>
    <p>{{ status }}</p>
    <div class="progress progress-striped active">
        <div class="progress-bar"  role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style=" width:{{ percent * 33.3333333 }}%">
            <span class="sr-only">0% Complete</span>
        </div>
    </div>
    <span><span class="gglyphicon glyphicon-certificate"></span>Veuillez saisir une commande pour connaitre son etat</span>
</div>
{% endblock %}

