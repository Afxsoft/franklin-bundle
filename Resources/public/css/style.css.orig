#nosproduits h5 {
    color: #404040;
    position: relative;
    display: inline-block;
    text-transform: uppercase;
    margin-bottom: 30px;
    font-weight: bold;
    font-size: 17px;
}
#nosproduits div:hover .service-icon {
    border: 10px solid #e96656;
}
#confort div:nth-child(3){
    background: -webkit-linear-gradient( left, #ebdac9 0%, #ececec 100%);
    background:    -moz-linear-gradient( left, #ebdac9 0%, #ececec 100%);
    background:     -ms-linear-gradient( left,#ebdac9 0%, #ececec 100%);
    background:      -o-linear-gradient(left,#ebdac9 0%, #ececec 100%);
    background:         linear-gradient( left, #ebdac9 0%, #ececec 100%);
    padding: 14px 23px;
}
#content{
    margin-bottom: 40px;
}
#liste-produits div img{
    max-height: 213px;
}
.dark-text {
    color: #404040;
}
#content {
    text-align: center; 
}
#liste-produits .row{
    margin-top: 15px;
}
.service-icon {
    margin-bottom: 30px;
    width: 145px;
    height: 145px;
    margin: auto;
    border-radius: 50%;
    border: 10px solid #ececec;
    margin-bottom: 20px;
    position: relative;
    -webkit-transition: all 0.2s ease-in-out;
    transition: all 0.2s ease-in-out;
}
.pixeden {
    height: 63px;
    margin: auto;
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    display: block;
}

i.conseil{
    background: url(../images/conseil.png) no-repeat;
    width: 68px;
}
i.produit{
    background: url(../images/produits.png) no-repeat;
    width: 68px;
}
i.installation{
    background: url(../images/installation.png) no-repeat;
    width: 68px;
}

/*     HEADER
---------------------*/
.navbar{
    background: #fff;
}
#header  .logo-top{
    background: url('../images/logo.png') no-repeat;
    display: block;
    font-size: 0;
    height: 76px;
    width: 154px;
    position: relative;
    top: 5px;

}
#header {
    left: 0;
    position: relative;
    right: 0;
    top: 0;
    z-index: 20;
}
.top .navbar-inverse .navbar-nav>li>a {
    color: #404040;
    padding: 0;
    line-height: 35px;
}
.navbar-inverse .navbar-nav>li>a:hover {
    color: #e96656;
    outline: none;
}
.navbar-inverse .navbar-nav>li {
    display: inline;
    margin-right: 20px;
    margin-top: 20px;
}
