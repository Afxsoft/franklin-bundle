/*     GLOBAL
---------------------*/
body{
    font-family: 'Lato', sans-serif;
    font-size: 14px;
}
h1,h2{
    font-family: 'Montserrat', 'sans-serif';
    font-weight: 700;
    text-transform: uppercase;
}
h1{
    font-size: 55px;
}
h2{
    font-size: 45px ;
    padding-bottom: 10px; 
}
h6{
    font-size: 16px;
}
hr{
    border-top: #e19C56 solid 2px;
}
.navbar{
    border: none;
    margin-bottom: 0px;
}
